<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Until_Maintenance__c</fullName>
    <description>Number of days until next maintenance is due</description>
    <externalId>false</externalId>
    <formula>IF(
  ISBLANK(Next_Maintenance_Due__c),
  NULL,
  Next_Maintenance_Due__c - TODAY()
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Shows days remaining until maintenance is due (negative if overdue)</inlineHelpText>
    <label>Days Until Maintenance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
