<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Since_Last_Maintenance__c</fullName>
    <description>Formula field calculating days since last maintenance</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Last_Maintenance_Date__c), NULL, TODAY() - Last_Maintenance_Date__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Automatically calculated number of days since the last maintenance date</inlineHelpText>
    <label>Days Since Last Maintenance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
