<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Install_Date_Cannot_Be_Future</fullName>
    <active>true</active>
    <errorConditionFormula>InstallDate &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>InstallDate</errorDisplayField>
    <errorMessage>Installation date cannot be in the future</errorMessage>
</ValidationRule>
